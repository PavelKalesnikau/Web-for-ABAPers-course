<mvc:View 
    xmlns:mvc="sap.ui.core.mvc" 
    xmlns="sap.m">
    <Table items="{bd>/Suppliers}">
        <columns>
            <Column>
                <Text text="{i18n>SupplierID}" />
            </Column>
            <Column>
                <Text text="{i18n>CompanyName}" />
            </Column>
            <Column>
                <Text text="{i18n>ContactName}" />
            </Column>
            <Column>
                <Text text="{i18n>Address}" />
            </Column>
            <Column>
                <Text text="{i18n>City}" />
            </Column>
            <Column>
                <Text text="{i18n>Region}" />
            </Column>
             <Column>
                <Text text="{i18n>PostalCode}" />
            </Column>
             <Column>
                <Text text="{i18n>Country}" />
            </Column>
             <Column>
                <Text text="{i18n>Phone}" />
            </Column>
            <Column>
                <Text text="{i18n>Fax}" />
            </Column>
        </columns>
        <items>
            <ColumnListItem >
                <cells>
                    <Text text="{bd>SupplierID}" />
                    <Text text="{bd>CompanyName}" />
                    <Text text="{bd>ContactName}" />
                    <Text text="{bd>ContactTitle}" />
                    <Text text="{bd>Address}" />
                    <Text text="{bd>City}" />                    
                    <Text text="{bd>Region}" />                    
                    <Text text="{bd>PostalCode}" />                    
                    <Text text="{bd>Country}" />                    
                    <Text text="{bd>Phone}" />                    
                    <Text text="{bd>Fax}" />                    
                </cells>
            </ColumnListItem>
        </items>
    </Table>
</mvc:View>
